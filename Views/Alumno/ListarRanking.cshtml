@model X.PagedList.IPagedList<AlumnoModel>
@using PagedList.Mvc;
@using PagedList;

@{
    ViewData["Title"] = "ListarRanking";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

<h2 class="maintitle">Ranking de Puntaje de Alumnos</h2>

<div class="container principal">

    <div class="table">
        <table class="center-table">
            <thead>
                <tr class="bg-second">
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Apellido Paterno</th>
                    <th>Apellido Materno</th>
                    <th>Grupo</th>
                    <th>Puntaje</th>
                </tr>
            </thead>
            <tbody class="bg-secondor">
                @foreach (var item in Model)
                {
                    <tr>
                        <td> @item.IdAlumno </td>
                        <td> @item.Nombre </td>
                        <td> @item.ApPaterno </td>
                        <td> @item.ApMaterno </td>
                        <td> @item.Grupo </td>
                        <td> @item.Puntaje </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>